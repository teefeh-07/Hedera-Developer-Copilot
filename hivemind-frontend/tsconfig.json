{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "outDir": "out",
    "lib": ["ES2020", "DOM"],
    "sourceMap": true,
    "rootDir": ".",
    "strict": false,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": false,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "jsx": "react-jsx",
    "esModuleInterop": true
  },
  "exclude": ["node_modules", ".vscode-test"]
}
